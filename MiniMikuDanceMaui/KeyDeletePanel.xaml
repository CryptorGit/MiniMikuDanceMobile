<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MiniMikuDanceMaui.KeyDeletePanel"
             BackgroundColor="{StaticResource OverlayBackgroundColor}">
    <VerticalStackLayout Spacing="6" Padding="8">
        <Picker x:Name="BonePicker" Title="Bone" SelectedIndexChanged="OnBoneChanged" />
        <Picker x:Name="FramePicker" Title="Frame" SelectedIndexChanged="OnFrameChanged" />
        <HorizontalStackLayout HorizontalOptions="End" Spacing="10">
            <Button Text="delete" x:Name="DeleteButton" Clicked="OnDeleteClicked" />
            <Button Text="cancel" x:Name="CancelButton" Clicked="OnCancelClicked" />
        </HorizontalStackLayout>
    </VerticalStackLayout>
</ContentView>
