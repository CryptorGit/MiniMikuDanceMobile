<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MiniMikuDanceMaui.SettingPage"
             BackgroundColor="Transparent">

    <ContentPage.MenuBarItems>
        <MenuBarItem Text="File" />
        <MenuBarItem Text="Edit" />
        <MenuBarItem Text="Selection" />
        <MenuBarItem Text="View">
            <MenuFlyoutItem Text="HOME" Clicked="OnHomeClicked" />
            <MenuFlyoutItem Text="SETTING" Clicked="OnSettingClicked" />
        </MenuBarItem>
        <MenuBarItem Text="Help" />
    </ContentPage.MenuBarItems>
    <AbsoluteLayout x:Name="Root">
        <StackLayout x:Name="ContentLayout" VerticalOptions="Center" HorizontalOptions="Center" Spacing="20">
        </StackLayout>
    </AbsoluteLayout>
</ContentPage>
